{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AppConfig": {
    "IisApplication": "__Application_IIS_Param__"
  },

  "Jwt": {
    "Issuer": "__JwtIssuer__",
    "Key": "__JwtKey__"
  },

  "ConnectionStrings": {
    "Context": "__ConnectionString__"
  },

  "Security": {
    "Key": "__SecurityKey__"
  },

  "SwaggerConfiguration": {
    "Enable": "__SwaggerEnable__",
    "Title": "SGI",
    "Description": "API para o sistema SGI"
  },

  "Smtp": {
    "Host": "__SmtpHost__",
    "Port": "__SmtpPorta__",
    "Remetente": "__SmtpRemetente__",
    "Sac": "__SmtpSac__",
    "Password": "__SmtpSenha__",
    "Autenticacao": "__SmtpAutenticacao__",
    "ConnectionType": "__SmtpConnectionType__"
  }
}
